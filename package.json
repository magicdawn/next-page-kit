{
  "name": "next-page-kit",
  "version": "0.0.3",
  "description": "Helper for handle next-next page",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "test-cover": "nyc --reporter=lcov --reporter=text mocha",
    "report-cover": "codecov",
    "gen-readme": "swig render ./readme/readme.md > README.md && prettier --write README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/next-page-kit.git"
  },
  "keywords": [
    "next",
    "page",
    "next-next"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/next-page-kit/issues"
  },
  "homepage": "https://github.com/magicdawn/next-page-kit#readme",
  "dependencies": {
    "cheerio": "^0.22.0",
    "debug": "^4.1.1",
    "lodash": "^4.17.15",
    "umi-request": "^1.2.18"
  },
  "devDependencies": {
    "@magicdawn/eslint-config": "latest",
    "@magicdawn/prettier-config": "latest",
    "codecov": "^3.3.0",
    "eslint": "^6.8.0",
    "husky": "latest",
    "lint-staged": "latest",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "prettier": "latest",
    "promise.delay": "^0.0.1",
    "should": "^13.2.3"
  },
  "files": [
    "index.js",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,less,vue}": [
      "prettier --write",
      "git add"
    ]
  }
}
